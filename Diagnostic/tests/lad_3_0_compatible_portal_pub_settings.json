{
  "storageAccount": "gisactivitydiagnosticlog",
  "storageAccountName" : "gisactivitydiagnosticlog",
  "storageAccountSasToken": "__STORAGE_SAS_TOKEN__",
  "sink" : [
    {
      "name": "insights-telemetry-lad",
      "type": "EventHub",
      "sasURL": "__EVENT_HUB_SAS_TOKEN__"
    }
  ],
  "fileLogs": [
    {
      "file": "/var/log/audit/audit.log",
      "table": "varlogaudit"
    },
    {
      "file": "/var/log/messages",
      "table": "varlogmessages"
    },
    {
      "file": "/var/log/secure",
      "table": "varlogsecure"
    }
  ],
    "sampleRateInSeconds": 15
}
